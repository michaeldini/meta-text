{
  "name": "meta-text",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:fast": "vite build --sourcemap=false --minify=false",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:frontend": "npm run dev",
    "start:backend": "uvicorn backend.main:app --reload --host 0.0.0.0 --port 3001 --ssl-keyfile=certs/key.pem --ssl-certfile=certs/cert.pem",
    "start": "npm-run-all --parallel start:frontend start:backend",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "docs:api": "typedoc",
    "docs:serve": "npx http-server docs -p 3003",
    "docs:watch": "typedoc --watch",
    "docs:dev": "npm-run-all --parallel docs:watch docs:serve"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@mui/material": "^7.1.0",
    "html-react-parser": "^5.2.5",
    "loglevel": "^1.9.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.1",
    "react-window": "^1.8.11",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@faker-js/faker": "^9.8.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/ui": "^3.2.3",
    "dependency-cruiser": "^16.10.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "typedoc-theme-hierarchy": "^6.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
